    "dotenv": "^17.2.2",
    "electron": "^22.3.27",
    "electron-builder": "^23.6.0",
    "electron-is-dev": "^3.0.1",
    "fluent-ffmpeg": "^2.1.3",
    "framer-motion": "^12.23.18",
    "googleapis": "^160.0.0",
